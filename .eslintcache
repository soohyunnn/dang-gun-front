 post.price;\n    updatePost.content = post.content;\n    console.log(\"updatePost\", updatePost);\n    let formData1 = new FormData();\n    formData1.append(\"post\", JSON.stringify(updatePost));\n    for (let i = 0; i < file.length; i++) {\n      formData1.append(\"file\", file[i]);\n    }\n    if (validation()) {\n      updatePostAPI(\"/posts\", formData1).then((response) => {\n        console.log(\"updatePostAPI-Res\", response.status);\n        alert(\"게시글 수정이 완료되었습니다.\");\n      });\n    }\n  };\n\n  // useEffect(() => {\n  //   selectOnePostAPI(`/posts/${id}`, id).then((response) => {\n  //     console.log(\"selectOnePostAPI-Res\", response.data);\n  //     dispatch(savedetailpost(response.data));\n  //   });\n  // });\n\n  return (\n    <>\n      <HeaderContainer></HeaderContainer>\n      <article id=\"content\">\n        <form method=\"POST\" enctype=\"multipart/form-data\">\n          <section id=\"add-post\">\n            <h1 className=\"add-post-title\">게시글 수정</h1>\n            <input\n              className=\"title\"\n              name=\"title\"\n              type=\"text\"\n              placeholder=\"제목을 입력해 주세요.\"\n              onChange={onChange}\n              defaultValue={post.title}\n            />\n            <input\n              className=\"price\"\n              name=\"price\"\n              type=\"number\"\n              placeholder=\"금액을 입력해 주세요.(숫자만 입력가능)\"\n              onChange={onChange}\n              defaultValue={post.price}\n            />\n            <textarea\n              className=\"content\"\n              name=\"content\"\n              placeholder=\"내용을 입력하세요.\"\n              onChange={onChange}\n              defaultValue={post.content}\n            ></textarea>\n            <div className=\"upload-btn-wrapper\">\n              <div className=\"upload-name\">\n                <input\n                  id=\"filename\"\n                  value={fileObj !== \"\" ? fileObj : \"이미지를 업로드 해주세요.\"}\n                  disabled\n                ></input>\n                <button className={fileObj !== \"\" ? \"\" : \"delete-button\"}>\n                  X\n                </button>\n              </div>\n              <button className=\"btn\">Upload a file</button>\n              <input\n                type=\"file\"\n                id=\"myfile\"\n                className=\"file-hidden\"\n                multiple\n                onChange={fileupload}\n              />\n            </div>\n            <div>\n              <button\n                type=\"button\"\n                className=\"add_button\"\n                onClick={onClickUpdatePost}\n              >\n                수정\n              </button>\n            </div>\n          </section>\n        </form>\n      </article>\n      <FooterContainer></FooterContainer>\n    </>\n  );\n}\n\nexport default ModifiedContainer;\n",{"ruleId":"98","severity":1,"message":"99","line":10,"column":10,"nodeType":"100","messageId":"101","endLine":10,"endColumn":21},{"ruleId":"98","severity":1,"message":"102","line":15,"column":7,"nodeType":"100","messageId":"101","endLine":15,"endColumn":16},{"ruleId":"103","replacedBy":"104"},{"ruleId":"105","replacedBy":"106"},{"ruleId":"107","severity":1,"message":"108","line":69,"column":6,"nodeType":"109","endLine":69,"endColumn":10,"suggestions":"110"},{"ruleId":"98","severity":1,"message":"111","line":6,"column":10,"nodeType":"100","messageId":"101","endLine":6,"endColumn":21},{"ruleId":"98","severity":1,"message":"112","line":5,"column":10,"nodeType":"100","messageId":"101","endLine":5,"endColumn":24},{"ruleId":"98","severity":1,"message":"113","line":6,"column":8,"nodeType":"100","messageId":"101","endLine":6,"endColumn":15},{"ruleId":"98","severity":1,"message":"114","line":1,"column":17,"nodeType":"100","messageId":"101","endLine":1,"endColumn":26},{"ruleId":"98","severity":1,"message":"115","line":4,"column":3,"nodeType":"100","messageId":"101","endLine":4,"endColumn":14},"no-unused-vars","'Per